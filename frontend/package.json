{
  "name": "frontend",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "optimize-badges": "node ./scripts/optimize-badges.js",
    "prebuild": "npm run optimize-badges",
    "clean": "node -e \"const fs=require('fs');['.expo','node_modules/.cache','.metro','android','ios','dist','build'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\"",
    "clean:cache": "npm cache clean --force",
    "build:android:production": "eas build --platform android --profile production --clear-cache",
    "build:android:preview": "eas build --platform android --profile preview --clear-cache",
    "build:android:dev": "eas build --platform android --profile development --clear-cache",
    "build:android:apk": "npm run clean && eas build --platform android --profile preview --clear-cache",
    "build:ios:preview": "eas build --platform ios --profile preview --clear-cache",
    "build:ios:production": "eas build --platform ios --profile production --clear-cache",
    "build:ios:ipa": "npm run clean && eas build --platform ios --profile preview --clear-cache"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/auth": "^23.8.3",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.13.2",
    "expo": "^54.0.27",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.10",
    "expo-device": "^8.0.9",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.8",
    "expo-media-library": "~18.2.1",
    "expo-notifications": "^0.32.13",
    "expo-router": "~6.0.14",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-intersection-observer": "^0.2.1",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "sharp": "^0.33.5",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
